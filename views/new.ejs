<% include partials/header %>
<div class="container text-center" style="width: 30%; margin: 0 auto;">
        <h1>Create new CampGround</h1>
    <form action="/campgrounds" method='POST'>
        <div class="form-group">
            <input type="text" class="form-control" name="name" placeholder="Name">
        </div>
        <div class="form-group">
            <input type="text" class="form-control" name="image" placeholder="Image">
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary btn-lg btn-block">Sumbit!</button>
        </div>
        <a href="/campgrounds">Go back</a>
    </form>
</div>


